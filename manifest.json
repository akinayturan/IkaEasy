{
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "page_action": {
        "default_icon": "icon/128.png",
        "default_title": "ikariam"
    },
    "content_scripts": [
        {
            "js": [
                "langs/en.js",
                "langs/ru.js",
                "langs/de.js",
                "langs/he.js"
                "langs/tr.js"
            ],
            "matches": [
                "http://*.ikariam.gameforge.com/*",
                "https://*.ikariam.gameforge.com/*"
            ],
            "run_at": "document_start"
        },
        {
            "js": ["jquery.js"],
            "matches": [
                "http://*.ikariam.gameforge.com/*",
                "https://*.ikariam.gameforge.com/*",
                "http://ikalogs.ru/*",
                "http://*.ikalogs.ru/*"
            ],
            "run_at": "document_end"
        },
        {
            "js": [
                "zJS/utils.js",
                "zJS/lng.js",
                "zJS/var.js",
                "zJS/db.js",
                "init.js",
                "ikalogs.js"
            ],
            "matches": [
                "http://*.ikariam.gameforge.com/*",
                "https://*.ikariam.gameforge.com/*"
            ],
            "run_at": "document_end"
        },
        {
            "js": ["zJS/navigation.js"],
            "matches": [
                "http://*.ikariam.gameforge.com/*",
                "https://*.ikariam.gameforge.com/*"
            ],
            "run_at": "document_end"
        },
        {
            "js": [
                "page/__common.js",
                "page/city.js",
                "page/island.js",
                "page/resource.js",
                "page/militaryAdvisor.js",
                "page/militaryAdvisorReportView.js",
                "page/transport.js",
                "page/attack.js",
                "page/diplomacyAdvisor.js",
                "page/units.js",
                "page/topLists.js",
                "page/tavern.js",
                "page/options.js",
                "page/generalAttacksToAlly.js",
                "page/pirateFortress.js",
                "page/finances.js",
                "page/townHall.js"
            ],
            "matches": [
                "http://*.ikariam.gameforge.com/*",
                "https://*.ikariam.gameforge.com/*"
            ],
            "run_at": "document_end"
        },
        {
            "js": [
                "hotkeysPlugin.js",
                "page/sendIKMessage.js"
            ],
            "matches": [
                "http://*.ikariam.gameforge.com/*",
                "https://*.ikariam.gameforge.com/*"
            ],
            "run_at": "document_end"
        }
    ],
    "description": "Testing extension for ikariam.com.",
    "icons": {
        "128": "icon/128.png",
        "16": "icon/16.png",
        "32": "icon/32.png",
        "48": "icon/48.png"
    },

